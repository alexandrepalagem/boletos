        "node": true,
        "jest": true,
        "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": "eslint:recommended",
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": 2018,
        "sourceType": "module"
